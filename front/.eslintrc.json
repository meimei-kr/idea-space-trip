{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "prettier",
    "next"
  ],
  "ignorePatterns": ["node_modules", "dist"],
  "parserOptions": {
    "babelOptions": {
      "presets": ["next/babel"]
    }
  },
  "plugins": ["unused-imports"],
  "rules": {
    "unused-imports/no-unused-imports": "warn"
  }
}
